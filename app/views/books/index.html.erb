<h1>Books</h1>



<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Body</th>
            <th colspan="3"></th>
        </tr>
    </thead>
    <%@books.each do |book|%>
    <tbody>
        <tr>
            <td><%=book.title%></td>
            <td><%=book.body%></td>
            <td></td>
            <td></td>
            <td></td>

        </tr>
    </tbody>
    <% end %>
</table>

